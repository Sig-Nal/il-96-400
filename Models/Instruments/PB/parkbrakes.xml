<PropertyList>

  <path>switch.ac</path>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>switch</object-name>
  </animation>


    <animation>
        <type>rotate</type>
        <object-name>switch</object-name>
        <property>controls/gear/brake-parking</property>
        <factor>60</factor>
        <!--center>
            <x-m>-0.041</x-m>
            <y-m>-0.197</y-m>
            <z-m>0.417</z-m>
        </center-->
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>switch</object-name>
        <action>
            <button>0</button>
            <binding>
            <command>property-toggle</command>
            <property>controls/gear/brake-parking</property>
            </binding>
        </action>
    </animation>
 
</PropertyList>
